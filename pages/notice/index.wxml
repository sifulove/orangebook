<view class="page" style="width: 100%;height: 100%;">
  <scroll-view scroll-y style="height: 100%;" bindscrolltolower="lower">
    <view class="item" wx:for="{{list}}" wx:key="{{index}}" bindtap="navGo" data-url="{{item.src}}">
       <view class="imgContainer"><image mode="widthFill" src="../../image/notice.png"></image></view>
       <view class="msg">{{item.msg}}</view>
    </view>
  </scroll-view>
</view>
